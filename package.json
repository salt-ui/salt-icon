{
  "name": "salt-icon",
  "version": "3.7.6",
  "description": "tingle-icon component for tingle.",
  "repository": "http://gitlab.alibaba-inc.com/tingle-ui/tingle-icon",
  "author": "eternalsky",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "lib",
    "README.md"
  ],
  "keywords": [
    "tingle-icon",
    "Icon"
  ],
  "dependencies": {
    "classnames": "~2.2.3",
    "prop-types": "^15.6.1",
    "svg4everybody": "^2.1.8"
  },
  "devDependencies": {
    "fastclick": "^1.0.6",
    "react": "15.x",
    "react-dom": "15.x",
    "salt-tools": "~0.1.0",
    "to-case": "^2.0.0"
  },
  "scripts": {
    "start": "node genIconMap.js && salt-tools run icon-make-js && salt-tools run icon-build && salt-tools run start",
    "build": "salt-tools run icon-build",
    // 重要！！！发布后必须让乐高重新打包才能生效，否则将导致线上故障
    "pub": "salt-tools run icon-publish",
    "dep": "salt-tools run dep",
    "tnpm-dep": "salt-tools run tnpm-dep",
    "update": "salt-tools run update",
    "tnpm-update": "salt-tools run tnpm-update"
  },
  "license": "MIT",
  "screenshots": ""
}
